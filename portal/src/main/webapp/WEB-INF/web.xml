    <servlet-mapping>
        <servlet-name>getuserfeedbackbyAppwise</servlet-name>
        <url-pattern>/getuserfeedbackbyAppwise</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>LoginPage.jsp</welcome-file>
    </welcome-file-list>
</web-app>
